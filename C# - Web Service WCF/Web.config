<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.serviceModel>
    <behaviors>
      <endpointBehaviors>
        <behavior name="WebServiceJQuery.ServiceAspNetAjaxBehavior">
          <enableWebScript />
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true"
      multipleSiteBindingsEnabled="true" />
    <services>
      <service name="WebServiceJQuery.Service">
        <endpoint address="" behaviorConfiguration="WebServiceJQuery.ServiceAspNetAjaxBehavior"
          binding="webHttpBinding" contract="WebServiceJQuery.Service" />
      </service>
    </services>
  </system.serviceModel>
</configuration>