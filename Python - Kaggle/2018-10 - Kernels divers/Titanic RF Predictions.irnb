    {
      "metadata": {
        "_uuid": "15a03034092185c9a50433823263c06ce39ede58",
        "trusted": true,
        "_cell_guid": "726501b0-7fab-9b11-87b6-a5b2bd8a3638"
      },
      "cell_type": "code",
      "source": "# prediction\nrf.fitted = predict(rf_model)\nans_rf = rep(NA,891)\nfor(i in 1:891){\n  ans_rf[i] = as.integer(rf.fitted[[i]]) - 1\n}\n# Résultat\ntable(ans_rf)\n\nprint(rf_model)",
      "execution_count": null,
      "outputs": []
    },
